<title>Insert title here</title>
	<script>
		// $(document).ready(콜백함수);
		$(document).ready(function(){
			alert("djlkfsa");
			//$("#btn1").click(function(){});
			$("#btn1").click(function(){
				alert("나 클릭되었어 btn1");
			});
			//
			let btn2Tag = document.querySelector("#btn2");
			btn2Tag.addEventListener("click",function(){
				alert("나 클릭되었어 btn2");
			});
		});
	</script>
	
</head>
<body>
	<button id="btn1">버튼1</button>
