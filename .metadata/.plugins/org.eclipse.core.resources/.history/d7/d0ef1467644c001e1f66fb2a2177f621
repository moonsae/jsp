<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script>
	function send(){
		//alert("dsfdfs");
		
		//$.ajax();  ()인자정보를 객체타입으로 제공함
		$.ajax({
			type:"get",
			dataType:"text",
			url:"/ajaxPrj_09_6/food0",
			success: function(data, status){
				alert(data);
				console.log(data);
				console.log(status);
			},
			error: function(err){
				console.log(err);
			}
		}
		)
	}
</script>
</head>
<body>

   <button onclick="send()">음식정보</button>
   <div id="result"> </div>
   
